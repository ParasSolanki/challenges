    "start": "dotenvx run -f .env.production -- node dist/index.js"
  },
  "dependencies": {
    "@challenges/db": "workspace:^",
    "@hono/node-server": "^1.12.0",
    "@hono/zod-openapi": "^0.15.3",
    "@t3-oss/env-core": "^0.11.0",
    "hono": "^4.5.5",
    "oslo": "^1.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.7.0",
    "@types/node": "^20.11.17",
    "tsx": "^4.17.0",
    "esbuild": "^0.23.0"
  }
}
